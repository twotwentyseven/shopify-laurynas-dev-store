                    }, {
                        "type": "header",
                        "content": "Content Position"
                    }, {
                        "type": "select",
                        "id": "x_position_mob",
                        "options": [
                            {
                                "value": "left-1/2 translate-x-[-50%]",
                                "label": "Center"
                            }, {
                                "value": "left-0 right-auto",
                                "label": "Left"
                            }, {
                                "value": "right-0 left-auto",
                                "label": "Right"
                            }
                        ],
                        "label": "Horizontal postion (Mobile)",
                        "default": "left-1/2 translate-x-[-50%]"
                    }, {
                        "type": "select",
                        "id": "y_position_mob",
                        "options": [
                            {
                                "value": "top-1/2 translate-y-[-50%]",
                                "label": "Middle"
                            }, {
                                "value": "top-0",
                                "label": "Top"
                            }, {
                                "value": "bottom-0",
