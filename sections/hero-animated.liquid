<section class="hero | w-full relative">
    {% comment %} swiper settings are passed through as json and parsed in app.js, all handles need to be wrapped in double quotes. {% endcomment %}
    {%- comment -%}Section level overlay{%- endcomment -%}
        <div
        data-swiper
        data-swiper-settings='{"loop": false, "spaceBetween": 0, "centeredSlides": true, "speed": 500, "threshold": 20, "slidesPerView": 1, "autoplay": { "delay": 5000, "disableOnInteraction": true }, "pagination": { "el": ".hero .swiper-pagination", "clickable": true, "bulletClass": "rounded-sm mx-0.5 inline-block border h-2 w-6 leading-3 border-secondary-900 transition-all", "bulletActiveClass": "bg-secondary-900" }}'
        class="swiper hero-banner-swiper bg-primary-900"> <div class="swiper-wrapper ">

            {% for block in section.blocks %}
                {% assign bs = block.settings %}
                {% if block.type == "slide-animated-5-assets" %}
                    <div data-swiper-autoplay="10000" data-slide-color-scheme="{{ bs.carousel_pagination_color }}" class="swiper-slide | {{ bs.bg_color }}-{{ bs.bg_color_shade }} | p-container-gutter lg:p-container-gutter-lg w-full !h-auto min-h-[702px] | lg:min-h-[600px] | {% if section.settings.enable_thin %} {{ section.settings.thin_amount }} {%endif%}">
                        {%- comment -%}Block level overlay{%- endcomment -%}
                        {% if bs.bg_overlay %}
                            <div class="overlay | absolute top-0 left-0 w-full h-full -z-10 " style="background: var(--gradient-desktop, linear-gradient(270deg, rgba(255, 255, 255, 0.00) 22.92%, rgba(0, 0, 0, 0.30) 65.10%));"></div>
                        {% endif %}
                        <div class="animation-wrapper | absolute h-1/2 w-2/3 top-0 right-0 left-0 z-[2] mx-auto | lg:w-full lg:h-full lg:right-auto lg:left-auto lg:mx-0 lg:overflow-hidden" style="perspective:1000px; ">
                            {% if bs.image != blank %}
                            <div class="wrapper h-full w-full top-0 overflow-hidden z-[200] absolute | lg:right-0 lg:w-2/3 ">
                            {% comment %}
                                {% render 'responsive-image' with image: bs.image, mobile_image: bs.image_mob, image_class: 'image h-full w-full  pointer-events-none absolute object-contain', wrapper_class: 'party-animation image-wrapper-main-trigger h-full w-full top-0 overflow-hidden z-[200] scale-0  |  ' %}
                            {% endcomment %}
                                
                                <div class="party-animation image-wrapper-main-trigger h-full w-full top-0 overflow-hidden z-[200] scale-0  |  ">
                                    <img src="{{ bs.image | img_url: '800x' }}" class="image hidden lg:block h-full w-full  pointer-events-none absolute object-contain" alt="{{ bs.image.alt }}">
                                    <img src="{{ bs.image | img_url: '300x' }}" class="image block lg:hidden h-full w-full  pointer-events-none absolute object-contain" alt="{{ bs.image.alt }}">
                                </div>
                            </div>
                                
                            {% endif %}
                            {% if bs.image_party_one != blank %}
                            <div class="wrapper z-[1] right-[45vw] top-[40%] h-[14vw] w-[9vw] max-w-[130px] max-h-[180px] absolute | lg:right-[50vw]">
                                {% render 'responsive-image' with image: bs.image_party_one, mobile_image: bs.image_mob, image_class: 'image  pointer-events-none absolute top-0 right-0 bottom-0 left-0 h-full w-full object-contain ', wrapper_class: 'party-animation party-trigger-one | image-wrapper overflow-hidden w-full h-full ' %}
                            </div>
                            {% endif %}
                            {% if bs.image_party_two != blank %}
                            <div class="wrapper z-[1] right-[38vw] top-[40%] h-[14vw] w-[9vw] max-w-[130px] max-h-[180px] absolute | lg:right-[43vw]">
                                {% render 'responsive-image' with image: bs.image_party_two, mobile_image: bs.image_mob, image_class: 'image  pointer-events-none absolute top-0 right-0 bottom-0 left-0 h-full w-full object-contain ', wrapper_class: 'party-animation party-trigger-two | image-wrapper overflow-hidden  w-full h-full ' %}
                            </div>
                            {% endif %}
                            {% if bs.image_party_three != blank %}
                            <div class="wrapper z-[1] right-[29vw] top-[40%]  h-[14vw] w-[9vw] max-w-[130px] max-h-[180px] absolute | lg:right-[35vw]">
                                {% render 'responsive-image' with image: bs.image_party_three, mobile_image: bs.image_mob, image_class: 'image  pointer-events-none absolute top-0 right-0 bottom-0 left-0 h-full w-full object-contain ', wrapper_class: 'party-animation party-trigger-three | image-wrapper overflow-hidden  w-full h-full ' %}
                            </div>
                            {% endif %}
                            {% if bs.image_party_four != blank %}
                            <div class="wrapper z-[1] right-[21vw] top-[40%] h-[14vw] w-[9vw] max-w-[130px] max-h-[180px] absolute | lg:right-[27vw]">
                                {% render 'responsive-image' with image: bs.image_party_four, mobile_image: bs.image_mob, image_class: 'image   pointer-events-none absolute top-0 right-0 bottom-0 left-0 h-full w-full object-contain ', wrapper_class: 'party-animation party-trigger-four | image-wrapper overflow-hidden  w-full h-full ' %}
                            </div>
                            {% endif %}
                            {% if bs.image_party_five != blank %}
                            <div class="wrapper z-[1] right-[12vw] top-[40%] h-[14vw] w-[9vw] max-w-[130px] max-h-[180px] absolute | lg:right-[18vw]">
                                {% render 'responsive-image' with image: bs.image_party_five, mobile_image: bs.image_mob, image_class: 'image   pointer-events-none absolute top-0 right-0 bottom-0 left-0 h-full w-full object-contain ', wrapper_class: 'party-animation party-trigger-five | image-wrapper overflow-hidden  w-full h-full ' %}
                            </div>
                            {% endif %}
                        </div>

                        <div class="lg:left-0 lg:translate-x-0 lg:top-1/2 top-1/2 translate-y-[-35%] {{ bs.text-align }} left-1/2 translate-x-[-50%] absolute w-full max-w-2xl p-container-gutter z-[3] h-fit top-[calc(50%+16vw)] | lg:p-container-gutter-lg lg:translate-y-[-50%] lg:w-1/2">

                            {% if bs.title != blank %}
                                <{{bs.title_tag}} class="title | mb-4 |  {{ bs.title_typeface }} {{ bs.title_weight }} {{ bs.title_color }}-{{ bs.title_color_shade }} {{ bs.title_size }} | lg:mb-10">
                                    {{ bs.title }}
                                </{{bs.title_tag}}>
                            {% endif %}

                            {% if bs.subtitle != blank %}
                                <div class="subtitle | mb-6 | {{ bs.subtitle_typeface }} {{ bs.subtitle_weight }} {{ bs.subtitle_color }}-{{ bs.subtitle_color_shade }} {{ bs.subtitle_size }}">
                                    {{ bs.subtitle }}
                                </div>
                            {% endif %}

                            <div class="btn-wrapper | -mx-gutter flex {{ bs.text-align }}">

                                {% if bs.btn_url != blank and bs.btn_text != blank %}
                                    {% render 'button' element: 'a', type: '', href: bs.btn_url, target: '',   border_color: bs.btn_border_color, border_color_shade: bs.btn_border_color_shade, border_width: 'border', bg_color: bs.btn_color, bg_color_shade: bs.btn_color_shade, text_color: bs.btn_text_color, text_color_shade: bs.btn_text_color_shade, hover_bg: bs.btn_hover_color, hover_bg_shade: bs.btn_hover_color_shade, hover_border_color: bs.btn_hover_border_color, hover_border_color_shade: bs.btn_hover_border_color_shade, hover_text: bs.btn_hover_text_color, hover_text_shade: bs.btn_hover_text_color_shade, content: bs.btn_text, custom_class: 'mx-gutter rounded-lg w-fit py-3 font-headline lg:w-auto ', before_icon: '', after_icon: '' %}
                                {% endif %}

                                {% if bs.alt_btn_url != blank and bs.alt_btn_text != blank %}
                                    {% render 'button' element: 'a', type: '', href: bs.alt_btn_url, target: '',  border_color: bs.alt_btn_border_color, border_color_shade: bs.alt_btn_border_color_shade, border_width: 'border', bg_color: bs.alt_btn_color, bg_color_shade: bs.alt_btn_color_shade, text_color: bs.alt_btn_text_color, text_color_shade: bs.alt_btn_text_color_shade, hover_bg: bs.alt_btn_hover_color, hover_bg_shade: bs.alt_btn_hover_color_shade, hover_border_color: bs.alt_btn_hover_border_color, hover_border_color_shade: bs.alt_btn_hover_border_color_shade, hover_text: bs.alt_btn_hover_text_color, hover_text_shade: bs._alt_btn_hover_text_color_shade content: bs.alt_btn_text, custom_class: 'mx-gutter font-headline py-3 rounded-lg w-fit ', before_icon: '', after_icon: '' %}
                                {% endif %}

                            </div>
                            {% comment %}
                                data:'newsletter-trigger'
                            {% endcomment %}
                        </div>
                    </div>

                {% elsif block.type == "general-slide" %}
                    <div data-slide-color-scheme="{{ bs.carousel_pagination_color }}" class="swiper-slide | {{ bs.bg_color }}-{{ bs.bg_color_shade }} | p-container-gutter lg:p-container-gutter-lg w-full min-h-[702px] | lg:min-h-[600px] | {% if section.settings.enable_thin %} {{ section.settings.thin_amount }} {%endif%}">
                        {% if bs.bg_overlay %}
                            <div class="overlay | absolute top-0 left-0 w-full h-full z-[3]" style="background: var(--gradient-desktop, linear-gradient(270deg, rgba(255, 255, 255, 0.00) 22.92%, rgba(0, 0, 0, 0.30) 65.10%));"></div>
                        {% endif %}
                        {% if bs.video != blank %}
                            <div class="video-wrapper overflow-hidden pointer-events-none absolute h-full w-full left-0 top-0 z-[2] md:pt-[56.25%] pt-[133%]">
                                {{ bs.video | video_tag: image_size: '1440x', autoplay: true, loop: true, muted: true, controls: false, class: 'w-full h-full absolute top-1/2 left-1/2 -translate-x-[50%] -translate-y-[50%]' }}
                            </div>
                        {% else %}
                            {% if bs.vimeo_id != blank %}
                                <div class="video-wrapper overflow-hidden pointer-events-none absolute h-full w-full left-0 top-0 z-[2] md:pt-[56.25%] ">

                                    <iframe
                                        id="vimeo-{{ block.id }}"
                                        class=" w-[177.77777778vh] h-[56.25vw] min-h-full min-w-full absolute top-1/2 left-1/2 -translate-x-[50%] -translate-y-[50%] | lg:w-full lg:h-full lg:min-w-[auto] lg:min-h-[auto]"
                                        src=""
                                        frameborder="0"
                                        allow="autoplay; fullscreen"
                                        allowfullscreen
                                        loading="lazy"
                                        ></iframe>

                                </div>

                                <script>

                                    window.addEventListener('DOMContentLoaded', function() {
                                        mobileVideo = "{{ bs.vimeo_id_mob }}" || false
                                        const video_{{ block.id | replace: '-', '_' }} = document.querySelector('#vimeo-{{ block.id }}');
                                        function whichVideo() {
                                            if (window.innerWidth < 1024 && mobileVideo) {
                                                video_{{ block.id | replace: '-', '_' }}.setAttribute('src', 'https://player.vimeo.com/video/{{ bs.vimeo_id_mob }}?api=1&player_id=banner_player&autopause=0&autoplay=1&badge=0&byline=0&portrait=0&title=0&background=1&loop=1&muted=1');
                                            } else {
                                                video_{{ block.id | replace: '-', '_' }}.setAttribute('src', 'https://player.vimeo.com/video/{{ bs.vimeo_id }}?api=1&player_id=banner_player&autopause=0&autoplay=1&badge=0&byline=0&portrait=0&title=0&background=1&loop=1&muted=1');
                                            }
                                        }

                                        whichVideo();

                                        window.addEventListener("resize", whichVideo);

                                    });
                                </script>

                            {% else %}

                                {% if bs.image != blank %}

                                    {% render 'responsive-image' with image: bs.image, focal_point: bs.image.presentation.focal_point, mobile_image: bs.image_mob, focal_point_mobile: bs.image_mob.presentation.focal_point, image_class: 'image object-cover h-full w-full', wrapper_class: 'image-wrapper absolute h-full w-full left-0 top-0 overflow-hidden z-[1]' %}

                                {% endif %}

                            {% endif %}

                        {% endif %}

                        <div class="{{ bs.x_position }} {{ bs.y_position }} {{ bs.x_position_mob }} {{ bs.y_position_mob }} {{ bs.text-align }} absolute w-full max-w-2xl p-container-gutter z-[3] h-fit | lg:p-container-gutter-lg">

                            {% if bs.title != blank %}
                                <{{bs.title_tag}} class="title | mb-4 |  {{ bs.title_typeface }} {{ bs.title_weight }} {{ bs.title_color }}-{{ bs.title_color_shade }} {{ bs.title_size }}">
                                    {{ bs.title }}
                                </{{bs.title_tag}}>
                            {% endif %}

                            {% if bs.subtitle != blank %}
                                <div class="subtitle | mb-6 | {{ bs.subtitle_typeface }} {{ bs.subtitle_weight }} {{ bs.subtitle_color }}-{{ bs.subtitle_color_shade }} {{ bs.subtitle_size }}">
                                    {{ bs.subtitle }}
                                </div>
                            {% endif %}

                            <div class="btn-wrapper | -mx-gutter flex {{ bs.text-align }}">

                                {% if bs.btn_url != blank and bs.btn_text != blank %}
                                    {% render 'button' element: 'a', type: '', href: bs.btn_url, target: '', border_color: bs.btn_border_color, border_color_shade: bs.btn_border_color_shade, border_width: 'border', bg_color: bs.btn_color, bg_color_shade: bs.btn_color_shade, text_color: bs.btn_text_color, text_color_shade: bs.btn_text_color_shade, hover_bg: bs.btn_hover_color, hover_bg_shade: bs.btn_hover_color_shade, hover_border_color: bs.btn_hover_border_color, hover_border_color_shade: bs.btn_hover_border_color_shade, hover_text: bs.btn_hover_text_color, hover_text_shade: bs.btn_hover_text_color_shade, content: bs.btn_text, custom_class: 'mx-gutter w-fit lg:w-auto font-headline py-3', before_icon: '', after_icon: '' %}
                                {% endif %}

                                {% if bs.alt_btn_url != blank and bs.alt_btn_text != blank %}
                                    {% render 'button' element: 'a', type: '', href: bs.alt_btn_url, target: '', border_color: bs.alt_btn_border_color, border_color_shade: bs.alt_btn_border_color_shade, border_width: 'border', bg_color: bs.alt_btn_color, bg_color_shade: bs.alt_btn_color_shade, text_color: bs.alt_btn_text_color, text_color_shade: bs.alt_btn_text_color_shade, hover_bg: bs.alt_btn_hover_color, hover_bg_shade: bs.alt_btn_hover_color_shade, hover_border_color: bs.alt_btn_hover_border_color, hover_border_color_shade: bs.alt_btn_hover_border_color_shade, hover_text: bs.alt_btn_hover_text_color, hover_text_shade: bs._alt_btn_hover_text_color_shade content: bs.alt_btn_text, custom_class: 'mx-gutter py-3 w-fit font-headline lg:w-auto', before_icon: '', after_icon: '' %}
                                {% endif %}

                            </div>

                        </div>
                    </div>


                {% endif %}

            {% endfor %}
        </div>
        <div class="swiper-pagination h-3 leading-3 !bottom-[5%] flex px-container-gutter | lg:!bottom-[15%] lg:px-container-gutter-lg"></div>

    </div>

</section>


<script>

    
    //window.addEventListener("beforeInit", showPage);
    //window.addEventListener("load", showPage);
    //document.addEventListener("DOMContentLoaded",() => {
        //setTimeout(showPage,1000)
        //console.log("haribo")
        //}
    //);

    setTimeout(showPage,500)
    
    function showPage() {
      const animationWrapper = document.querySelector(".animation-wrapper");
      animationWrapper.querySelector(".image-wrapper-main-trigger").classList.add("image-wrapper-main")
      animationWrapper.querySelector(".party-trigger-one").classList.add("party-one")
      animationWrapper.querySelector(".party-trigger-two").classList.add("party-two")
      animationWrapper.querySelector(".party-trigger-three").classList.add("party-three")
      animationWrapper.querySelector(".party-trigger-four").classList.add("party-four")
      animationWrapper.querySelector(".party-trigger-five").classList.add("party-five")
    }

</script>

{% schema %}
    {
        "name": "hero-animated",
        "settings": [
            {
                "type": "checkbox",
                "id": "enable_thin",
                "label": "Enable thin version of the hero banner",
                "default": false

            }, {
                "type": "select",
                "id": "thin_amount",
                "options": [
                    {
                        "value": "!min-h-[420px] lg:!min-h-[520px]",
                        "label": "Thin"
                    }, {
                        "value": "!min-h-[220px] lg:!min-h-[360px]",
                        "label": "Thinner"
                    }
                ],
                "label": "How thin?",
                "default": "!min-h-[420px] lg:!min-h-[520px]",
                "info": "Caution: thinner banners may not work well with animations or large amounts of copy so adjust accordingly"
            }
        ],
        "blocks": [
            {
                "type": "general-slide",
                "name": "General Slide",
                "settings": [
                    {
                        "type": "checkbox",
                        "id": "bg_overlay",
                        "label": "Enable background overlay?"
                    },
                    {
                        "type": "select",
                        "id": "carousel_pagination_color",
                        "options": [
                            {
                                "value": "light-mode",
                                "label": "Light"
                            }, {
                                "value": "dark-mode",
                                "label": "Dark"
                            }
                        ],
                        "label": "Crousel pagination color scheme",
                        "default": "dark-mode"
                    },
                    {
                        "type": "header",
                        "content": "background media"
                    },
                    {
                        "type": "image_picker",
                        "id": "image",
                        "label": "Image",
                        "info": "Recommended size: 1440x800px"
                    }, {
                        "type": "select",
                        "id": "img_x_position",
                        "options": [
                            {
                                "value": "lg:left-1/2 lg:translate-x-[-50%] lg:w-2/3",
                                "label": "Center"
                            }, {
                                "value": "lg:left-0 lg:w-2/3 ",
                                "label": "Left"
                            }, {
                                "value": "lg:right-0 lg:w-2/3 ",
                                "label": "Right"
                            }, {
                                "value": "lg:h-full lg:w-full lg:left-0",
                                "label": "Center (cover)"
                            }
                        ],
                        "default": "lg:h-full lg:w-full lg:left-0",
                        "label": "Image desktop horizontal postion"
                    }, {
                        "type": "select",
                        "id": "img_fit",
                        "options": [
                            {
                                "value": "object-cover",
                                "label": "Cover"
                            },
                            {
                                "value": "object-contain",
                                "label": "Contain"
                            },
                            {
                                "value": "object-none",
                                "label": "None"
                            },
                            {
                                "value": "object-scale-down",
                                "label": "Scale down"
                            }, {
                                "value": "object-fill",
                                "label": "Fill"
                            }
                        ],
                        "default": "object-cover",
                        "label": "Image fitting"
                    }, {
                        "type": "image_picker",
                        "id": "image_mob",
                        "label": "Image (mobile)",
                        "info": "Recommended size: 375x460"
                    }, {
                        "type": "video",
                        "id": "video",
                        "label": "A Shopify-hosted video"
                    }, {
                        "type": "text",
                        "id": "vimeo_id",
                        "label": "Video ID",
                        "info": "Add the vimeo ID from the video URL"
                    }, {
                        "type": "text",
                        "id": "vimeo_id_mob",
                        "label": "Video ID Mobile",
                        "info": "Add the vimeo ID from the video URL"
                    }, {
                        "type": "header",
                        "content": "background color (if media is not used)"
                    }, {
                        "type": "select",
                        "id": "bg_color",
                        "options": [
                            {
                                "value": "bg-primary",
                                "label": "Teal"
                            },
                            {
                                "value": "bg-secondary",
                                "label": "Brown"
                            },
                            {
                                "value": "bg-tertiary",
                                "label": "Beige"
                            },
                            {
                                "value": "bg-neutral",
                                "label": "White"
                            }, {
                                "value": "bg-salmon",
                                "label": "Salmon"
                            }, {
                                "value": "bg-yellow",
                                "label": "Yellow"
                            }, {
                                "value": "bg-blue",
                                "label": "Blue"
                            }, {
                                "value": "bg-orange",
                                "label": "Orange"
                            }, {
                                "value": "",
