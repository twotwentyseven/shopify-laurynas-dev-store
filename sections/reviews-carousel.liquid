{% assign ss = section.settings %}

<section class="grid-carousel | relative w-full px-container-gutter pt-10 pb-16 | lg:px-container-gutter-lg | {{ ss.text-align }} {{ ss.bg_color }}-{{ss.bg_color_shade}}">

    {% if ss.title != blank %}
        <h2 class="title | mb-2 |  {{ ss.title_typeface }} {{ ss.title_weight }} {{ ss.title_color }}-{{ ss.title_color_shade }} {{ ss.title_size }}">
            {{ ss.title }}
        </h2>
    {% endif %}

    {% if ss.subtitle != blank %}
        <div class="content | mb-6 | lg:mb-10 | {{ ss.subtitle_typeface }} {{ ss.subtitle_size }} {{ ss.subtitle_weight }} {{ ss.subtitle_color }}-{{ ss.subtitle_color_shade }}">
            {{ ss.subtitle }}
        </div>
    {% endif %}

    <div class="btn-wrapper | -mx-gutter flex {{ ss.text-align }}">

        {% if ss.btn_url != blank %}
            {% render 'button' element: 'a', type: '', href: ss.btn_url, target: '', border_color: ss.btn_border_color, border_color_shade: ss.btn_border_color_shade, border_width: 'border', bg_color: ss.btn_color, bg_color_shade: ss.btn_color_shade, text_color: ss.btn_text_color, text_color_shade: ss.btn_text_color_shade, hover_bg: ss.btn_hover_color, hover_bg_shade: ss.btn_hover_color_shade, hover_border_color: ss.btn_hover_border_color, hover_border_color_shade: ss.btn_border_hover_color_shade, hover_text: ss.btn_hover_text_color, hover_text_shade: ss.btn_hover_text_color_shade, content: ss.btn_text, custom_class: 'mx-gutter', before_icon: '', after_icon: '' %}
        {% endif %}

        {% if ss.alt_btn_url != blank %}
            {% render 'button' element: 'a', type: '', href: ss.alt_btn_url, target: '', border_color: ss.alt_btn_border_color, border_color_shade: ss.alt_btn_border_color_shade, border_width: 'border', bg_color: ss.alt_btn_color, text_color: ss.alt_btn_text_color, text_color_shade: ss.alt_btn_text_color_shade, hover_bg: ss.alt_btn_hover_color, hover_bg_shade: ss.alt_btn_hover_color_shade, hover_border_color: ss.alt_btn_hover_border_color, hover_border_color_shade: ss.alt_btn_border_hover_color_shade, hover_text: ss.alt_btn_hover_text_color, hover_text_shade: ss._alt_btn_hover_text_color_shade content: ss.alt_btn_text, custom_class: 'mx-gutter', before_icon: '', after_icon: '' %}
        {% endif %}

    </div>

    <div {% if ss.swiper %}data-swiperdata-swiper-settings='{"loop": true, "spaceBetween": 24, "centeredSlides": false, "speed": 500, "threshold": "20", "slidesPerView": {{ ss.mobile_columns }}, "breakpoints": { "1024": { "slidesPerView": {{ ss.columns }}, "spaceBetween": 24, "pagination": false, "loop": false }}, "autoplay": { "delay": 5000, "disableOnInteraction": false }, "draggable": true, "pagination": { "el": ".grid-carousel .swiper-pagination", "clickable": true, "bulletClass": "rounded-full mx-0.5 inline-block border h-2 w-2 leading-3 {{ ss.title_color | replace: "text", "border" }}-{{ ss.title_color_shade }}", "bulletActiveClass": "{{ ss.title_color | replace: "text", "bg" }}-{{ ss.title_color_shade }}" }}'{% endif %} class="{% if ss.swiper %}swiper{% endif %}">

        <div class="{% if ss.swiper %}swiper-wrapper{% else %}flex flex-wrap{% endif %}">

            {% for block in section.blocks %}

                {% assign bs = block.settings %}

                <div class="{% if ss.swiper %}swiper-slide{% endif %} | mb-4 px-gutter {% if ss.mobile_columns == 1 %}w-full{%- else -%}w-1/{{- ss.mobile_columns -}}{% endif %} | lg:mb-0 {% if ss.columns == 1 %}lg:w-full{%- else -%}lg:w-1/{{- ss.columns -}}{%- endif -%}">

                    {% if bs.image != blank %}

                        {% render 'responsive-image' with image: bs.image, mobile_image: bs.image_mob, image_class: 'image mx-auto', wrapper_class: 'image-wrapper w-full mb-4' %}

                    {% endif %}

                    <div class="{{ ss.text-align }}">

                        {% if bs.content != blank %}
                            <div class="content | {{ bs.content_typeface }} {{ bs.content_weight }} {{ bs.content_size }} {{ bs.content_color }}-{{ bs.content_color_shade }}">
                                {{ bs.content }}
                            </div>
                        {% endif %}

                        {% if bs.reviewer != blank %}
                            <div class="reviewer | mt-4 {{ bs.reviewer_typeface }} {{ bs.reviewer_weight }} {{ bs.reviewer_color }}-{{ bs.reviewer_color_shade }} {{ bs.reviewer_size }}">
                                {{ bs.reviewer }}
                            </div>
                        {% endif %}

                        <div class="btn-wrapper | -mx-gutter flex {{ ss.text-align }}">

                            {% if bs.btn_url != blank %}
                                {% render 'button' element: 'a', type: '', href: bs.btn_url, target: '', border_color: bs.btn_color, border_width: 'border', bg_color: bs.btn_color, bg_color_shade: bs.btn_color_shade, text_color: bs.btn_text_color, text_color_shade: bs.btn_text_color_shade, hover_bg: bs.btn_hover_color, hover_bg_shade: bs.btn_hover_color_shade, hover_text: bs.btn_hover_text_color, hover_text_shade: bs.btn_hover_text_color_shade, content: bs.btn_text, custom_class: 'mx-gutter mt-4', before_icon: '', after_icon: '' %}
                            {% endif %}

                        </div>

                    </div>
                </div>

            {% endfor %}
        </div>

    </div>

    {% if ss.swiper %}
        <div class="swiper-pagination !bottom-8" style="--swiper-pagination-bullet-width: 12px;
                                                                                                                                                                                                        --swiper-pagination-bullet-height: 12px;
                                                                                                                                                                                                        --swiper-pagination-bullet-inactive-color: #fff;
                                                                                                                                                                                                        --swiper-pagination-color: #fff;"></div>
    {% endif %}

</section>


